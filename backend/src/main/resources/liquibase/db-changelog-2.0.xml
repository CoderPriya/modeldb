<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
        logicalFilePath="src/main/resources/liquibase/db-changelog-2.0.xml">

    <changeSet author="anandJ" id="C1">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="C1"/>
            </not>
        </preConditions>
        <createTable tableName="C1">
            <column name="id" type="int8" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true" nullable="false"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="C1"/>
        </rollback>
    </changeSet>

    <changeSet id="db_version_1.1" author="anandJ">
        <tagDatabase tag="db_version_1.1"/>
    </changeSet>

    <changeSet author="anandJ" id="C2">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="C2"/>
            </not>
        </preConditions>
        <createTable tableName="C2">
            <column name="id" type="int8" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true" nullable="false"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="C2"/>
        </rollback>
    </changeSet>

    <changeSet id="db_version_1.2" author="anandJ">
        <tagDatabase tag="db_version_1.2"/>
    </changeSet>

</databaseChangeLog>
